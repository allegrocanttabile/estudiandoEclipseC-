#include <iostream>
using namespace std;

int main() {

	float a, b, c, d, e, f;

	float resultado;

	cout<<"Ingrese el valor de a: ";
	cin>>a;
	cout<<"Ingrese el valor de b: ";
	cin>>b;
	cout<<"Ingrese el valor de c: ";
	cin>>c;
	cout<<"Ingrese el valor de e: ";
	cin<<e;
	cout<<"Ingrese el valor de f: ";
	cin<<f;

	resultado = a+(b/c) / d+(e/f);

	cout<<"El resultado es: "<<resultado;



	return 0;
}
