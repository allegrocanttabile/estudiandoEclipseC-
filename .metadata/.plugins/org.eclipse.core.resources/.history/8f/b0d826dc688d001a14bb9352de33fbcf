#include <iostream>
using namespace std;

int main() {
	int n1, n2, n3, promedio;

	cout<<"Ingrese la nota 01: ";cin>>n1;
	cout<<"Ingrese la nota 02: ";cin>>n2;
	cout<<"Ingrese la nota 03: ";cin>>n3;

	promedio = (n1+n2+n3)/3;

	cout<<""<<promedio<<endl;


	return 0;
}
