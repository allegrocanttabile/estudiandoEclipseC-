#include <iostream>
using namespace std;

int main() {
	float a, b, c, d, resultado=0;

	cout<<"Ingrese valor a: ";cin>>a;
	cout<<"Ingrese valor b: ";cin>>b;
	cout<<"Ingrese valor c: ";cin>>c;
	cout<<"Ingrese valor d: ";cin>>d;

	resultado=a+(b(c-d));

	cout<<"El Resultado: "<<resultado<<endl;


	return 0;
}
